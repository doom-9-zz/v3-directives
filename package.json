{
  "name": "v3-directives",
  "version": "0.0.4",
  "files": [
    "es",
    "lib",
    "README.md"
  ],
  "main": "lib/main.js",
  "module": "es/main.js",
  "exports": {
    ".": {
      "import": "./es/main.js",
      "require": "./lib/main.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build-gulp": "vue-tsc --noEmit && vite build -c ./vite.config.gulp.ts",
    "build-tsc-esm-lib": "vue-tsc --noEmit && tsc -b --force ./code/tsconfig.esm.json && tsc -b --force ./code/tsconfig.lib.json",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "highlight.js": "^11.4.0",
    "v3-directives": "^0.0.2",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.tsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --parser=vue --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write --parser markdown --prose-wrap never"
    ]
  },
  "sideEffects": false,
  "homepage": "https://v3-directives.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/doom-9/v3-directives"
  },
  "keywords": [
    "typescript",
    "directives Collection",
    "vue",
    "directive",
    "vue3"
  ]
}
